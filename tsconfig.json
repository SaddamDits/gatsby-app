{
    "compilerOptions": {
      "target": "ES6",
      "lib": ["DOM", "ES6"],
      "allowJs": true,
      "jsx": "react",
      "moduleResolution": "node",
      "strict": true,
      "noImplicitAny": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*.ts", "src/**/*.tsx"]
  }
  